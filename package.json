{
  "name": "electron-vue-ts-study",
  "version": "0.0.0",
  "author": "PentaTea <2490445193@qq.com>",
  "description": "An electron-vue project",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "yarn compile && electron-builder",
    "build:dir": "yarn build -- --dir -c.compression=store -c.mac.identity=null",
    "build:clean": "del dist/electron/* !.gitkeep",
    "build:web": "webpack --env.production --config .electron-vue webpack.web.config.js",
    "compile": "electron-webpack",
    "dev": "electron-webpack dev",
    "test": "npm run unit",
    "unit": "karma start test/unit/karma.conf.js",
    "postinstall": ""
  },
  "build": {
    "productName": "electron-vue-ts-study",
    "appId": "org.simulatedgreg.electron-vue",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.16.1",
    "babili-webpack-plugin": "^0.1.2",
    "copy-webpack-plugin": "^6.0.3",
    "source-map-support": "^0.4.17",
    "ts-loader": "3.5",
    "vue": "^2.6.11",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.0",
    "cross-env": "^5.0.5",
    "del-cli": "^1.1.0",
    "devtron": "^1.4.0",
    "electron": "^13.6.6",
    "electron-builder": "^22.7.0",
    "electron-debug": "^1.4.0",
    "electron-webpack": "^1.4.2",
    "electron-webpack-ts": "^1.1.2",
    "electron-webpack-vue": "^1.0.7",
    "inject-loader": "^3.0.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.31",
    "karma-webpack": "^2.0.1",
    "mocha": "^3.0.2",
    "typescript": "^3.9.6",
    "vue-loader": "^12.2.2",
    "webpack": "^3.5.2",
    "webpack-merge": "^4.1.0"
  }
}
